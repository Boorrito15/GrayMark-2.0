
<div class = "cards">
  <% @week_menus.each do |week_menu| %>
    <div class = "card">
      <div class ="card-body">
        <% week_menu.profile.allergy_profiles.each do |allergy_profile| %>
        <h5> <%= allergy_profile.ingredient.name.capitalize %>
        <% end %> free </h5>
        <%= link_to "View menu", week_menu_path(week_menu) %>
      </div>
    </div>
  <% end %>
</div>
